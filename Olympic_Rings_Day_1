let BulletPoint;
let Target;

// WAS: function setup() { ... }
setup = function () {
  createCanvas(800, 600);
  BulletPoint = ellipse(350, 315, 40, 40);
}
{Target = ellipse(350, 315, 120, 120);
}


// WAS: function draw() { ... }
draw = function () {
  background(220);
  BulletPoint = ellipse(350, 315, 40, 40);
  
}
{Target = ellipse(350, 315, 120, 120);
}
